{
  "name": "mimir",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/grasword/mimir#readme",
  "bugs": {
    "url": "https://github.com/grasword/mimir/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grasword/mimir.git"
  },
  "license": "MIT",
  "author": "Oleh Zhmaiev (https://github.com/grasword)",
  "bin": {
    "mimir": "./bin/run.mjs"
  },
  "type": "module",
  "exports": "./build/index.js",
  "typings": "./build/index.d.ts",
  "files": [
    "bin",
    "build"
  ],
  "scripts": {
    "prebuild": "rimraf build",
    "build": "run-p build:*",
    "postbuild": "run-s fix:alias",
    "build:swc": "swc ./source -d ./build",
    "build:types": "tsc --project tsconfig.build.json",
    "check": "run-p check:*",
    "check:code": "eslint . --ext .ts",
    "check:format": "prettier --check 'source/**/*.ts'",
    "fix": "run-s fix:code fix:format",
    "fix:alias": "tsc-alias -p tsconfig.build.json",
    "fix:code": "run-s 'lint -- --fix'",
    "fix:format": "prettier --write 'source/**/*.ts'",
    "prepublishOnly": "run-s build",
    "preversion": "run-s check test build",
    "test": "vitest run",
    "test:integration": "vitest run integration --coverage",
    "test:unit": "vitest run unit --coverage"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "dependencies": {
    "@e965/xlsx": "0.20.0",
    "@oclif/core": "3.0.0-beta.16",
    "@oclif/plugin-help": "5.2.19",
    "@oclif/plugin-plugins": "3.8.0",
    "debug": "4.3.4",
    "fast-glob": "3.3.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@2bad/tsconfig": "2.0.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.89",
    "@types/debug": "4.1.9",
    "@types/node": "20.6.5",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitest/coverage-v8": "0.34.5",
    "chokidar": "3.5.3",
    "eslint": "8.50.0",
    "eslint-config-oclif": "5.0.0",
    "eslint-config-oclif-typescript": "2.0.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vitest": "0.3.1",
    "npm-run-all": "4.1.5",
    "oclif": "3.17.1",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.5"
  },
  "engines": {
    "node": ">=18"
  },
  "volta": {
    "node": "20.7.0"
  },
  "oclif": {
    "commands": "./build/commands"
  }
}
